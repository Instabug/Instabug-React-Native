{
  "name": "instabug-reactnative",
  "description": "React Native plugin for integrating the Instabug SDK",
  "version": "11.3.0",
  "author": "Instabug (https://instabug.com)",
  "repository": "github:Instabug/Instabug-React-Native",
  "homepage": "https://www.instabug.com/platforms/react-native",
  "bugs": "https://github.com/Instabug/Instabug-React-Native/issues",
  "license": "MIT",
  "main": "src/index",
  "types": "src/index.d.ts",
  "keywords": [
    "react-native",
    "instabug",
    "debugging",
    "errors",
    "exceptions",
    "logging",
    "reporting",
    "feedback"
  ],
  "scripts": {
    "test": "jest",
    "build": "tsc"
  },
  "peerDependencies": {
    "react": ">=16.8.6",
    "react-native": ">=0.60.0"
  },
  "devDependencies": {
    "@types/node": "^18.7.23",
    "@types/react-native": "^0.66.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^24.8.0",
    "esprima": "^4.0.1",
    "jest": "^24.8.0",
    "metro-react-native-babel-preset": "0.72.2",
    "nock": "^13.2.9",
    "react": "^16.8.6",
    "react-native": "^0.60.0",
    "typescript": "^4.8.4",
    "wait-for-expect": "^1.2.0",
    "xhr2": "^0.2.1"
  },
  "jest": {
    "preset": "react-native",
    "clearMocks": true,
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "./src/**/*.(js|ts)"
    ],
    "setupFilesAfterEnv": [
      "./tests/setup.js"
    ],
    "modulePathIgnorePatterns": [
      "example"
    ]
  }
}
