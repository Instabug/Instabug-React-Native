{
  "name": "instabug-react-native-workspace",
  "version": "1.0.0",
  "private": true,
  "repository": "git@github.com:Instabug/Instabug-React-Native.git",
  "license": "MIT",
  "scripts": {
    "example": "yarn --cwd examples/default",
    "core": "yarn --cwd core",
    "ndk": "yarn --cwd ndk",
    "pods": "cd examples/default && pod-install --quiet",
    "bootstrap": "yarn && yarn core && yarn ndk && yarn example && yarn pods",
    "bump": "./scripts/bump-up.sh"
  },
  "dependencies": {
    "@instabug/danger-plugin-coverage": "Instabug/danger-plugin-coverage",
    "danger": "^11.2.5",
    "pod-install": "^0.1.38"
  },
  "devDependencies": {
    "typescript": "^4.8.4"
  }
}